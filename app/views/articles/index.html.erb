





    <!-- Page Content -->
    <div class="container" data-controller="hello">

      <div class="row">

        <!-- Blog Entries Column -->
        <div class="col-md-12">



        <% @articles.each do |article|%>
            <!-- Third Blog Post -->
            <h2 class="post-title">
              <a href="post.html"><%=article.title%></a>
            </h2>
            <p class="lead">
              by Author
            </p>
            <p><span class="glyphicon glyphicon-time"></span> Posted on August 28, 2013 at 10:45 PM</p>
            <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Cupiditate, voluptates, voluptas dolore ipsam cumque quam veniam accusantium laudantium adipisci architecto itaque dicta aperiam maiores provident id incidunt autem. Magni, ratione.</p>
            <a class="" href="post.html">Read More</a>
            <%=link_to "Read More", article_path(article.id), :class => "btn btn-default" %>

            <hr>
        <%end%>

          <!-- Pager -->
          <ul class="pager">
            <li class="previous">
              <a href="#">Prev</a>
            </li>
            <li class="next">
              <a href="#">Next</a>
            </li>
          </ul>

<div class="container">
  <article class="articles">
    <% @articles.each do |article|%>
      <article class="article-info">
        <div class="author-info">
          <div class="profile-pic">
            <img src="data:image/png;base64," alt="Avatar" class="avatar">
          </div>
          <div class="author-writter">
            <div class="author-name">
              <%=article.user.firstName%> <%=article.user.lastName%>
            </div>
            <div class="date-written">
              <%=article.created_at%>
            </div>
          </div>
        </div>
        <div class="align">
          <div class="title">
            <%= link_to article.title, article_path(article.id) %>
          </div>
          <div class="image-parent">
            <img src="https://www.shutterstock.com/image-photo/newspapers-laptop-pile-daily-papers-260nw-1408220747.jpg"
            alt=""
            class="image-child">
          </div>
          <div class="tags">
            <div class="tag">
              <a href="">#play</a>
            </div>
            <div class="tag">
              <a href="">#software</a>
            </div>
            <div class="tag">
              <a href="">#webdev</a>
            </div>
          </div>
        </div>

        <div class="supliments">
          <div class="reactions">
              <a href="" id= "reaction"><i class="fa fa-heart"> 8 reactions</i></a>
              <a href="" id= "reaction"><i class="far fa-comment"> 37 comments</i></a>
              <a href="" id= "reaction"> 5min read</a>
              <a href="" id= "reaction"> <i class="fa-solid fa-bookmark"></i></a>
          </div>
        </div>
      </article>

    <%end%>

  </article>
</div>
<div class="digg_pagination">
      <%= paginate @articles %>
</div>
